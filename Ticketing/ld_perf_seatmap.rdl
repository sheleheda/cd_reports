  <Code>public Shared Function GetHexColor(LongColor As Integer) As String
   Dim Red As String
   Dim Green As String
   Dim Blue As String
   Dim HexColor As String
	
	Red = Hex(LongColor Mod 256)
   	Green = Hex(Fix(LongColor/256) Mod 256)
	Blue = Hex(Fix(LongColor/256/256) Mod 256)
 
	If len(Red) = 1
		Red = "0" &amp; Red
	End If
	If len(Green) = 1
		Green = "0" &amp; Green
	End If
	If len(Blue) = 1
		Blue = "0" &amp; Blue
	End If
	
 	HexColor = "#" &amp; Red &amp; Green &amp; Blue
    Return HexColor
End Function

Function GetTessituraLink(resource as String, id as Long) As String
	If NOT Report.Globals!RenderFormat.IsInteractive Then 
		RETURN Nothing
	End IF
	If NOT (len(resource) &gt; 0) OR id &lt; 1 Then
		RETURN Nothing
	End If
	
	RETURN Report.Globals!ReportServerUrl &amp; "?/Tessitura/Links/" &amp; resource &amp; "&amp;Id=" &amp; id
End Function
